    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Dictionary Generator",
            "request": "launch",
            "mainClass": "eg.edu.guc.csen.dictionarygenerator.App",
            "projectName": "dictionary-generator",
            "cwd": "${workspaceFolder}/dictionary-generator",
            "console": "internalConsole",
            "args": [
                "${workspaceFolder}/translator/src/main/resources/identifiers.json",
            ]
        },
        {
            "type": "java",
            "preLaunchTask": "",
            "name": "Demo Arabic",
            "request": "launch",
            "mainClass": "fwldr.tjrb_x1E97_",
            "projectName": "demo-ar",
            "cwd": "${workspaceFolder}/examples/demo-ar",
            "console": "internalConsole"
        },
        {
            "type": "java",
            "name": "Ecliplse Plugin",
            "request": "launch",
            "mainClass": "org.eclipse.equinox.launcher.Main",
            "args": [
                "-launcher", "${env:USERPROFILE}\\eclipse\\java-2022-12\\eclipse\\eclipse.exe",
                "-name", "Eclipse",
                "--showsplash", "600",
                "-product", "org.eclipse.epp.package.java.product",
                "-configuration", "${workspaceFolder}\\.metadata\\.plugins\\org.eclipse.pde.core\\Eclipse Application",
                "-data", "file:${workspaceFolder}\\..\\runtime-EclipseApplication",
                "-dev", "file:${workspaceFolder}\\.metadata\\.plugins\\org.eclipse.pde.core\\Eclipse Application\\dev.properties",
                "-os", "win32",
                "-ws", "win32",
                "-arch", "x86_64",
                "-nl", "en_GB",
                "-consoleLog"
            ],
            "classPaths": [
                "${env:USERPROFILE}\\.p2\\pool\\plugins\\org.eclipse.equinox.launcher_1.6.400.v20210924-0641.jar"
            ],
            "vmArgs": [
                "-Dorg.eclipse.swt.graphics.Resource.reportNonDisposed=true",
                "-Declipse.pde.launch=tru-Declipse.pde.launch=true",
                "--add-modules=ALL-SYSTEM",
                "-Djava.security.manager=allow",
                "-Dfile.encoding=UTF-8",
            ]
        }
    ]
}